<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="custom_backend_assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/force_timezone/static/src/js/session.js"/>
        </xpath>
    </template>

    <template id="assets_common" name="assets_common" inherit_id="web.assets_common">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/force_timezone/static/lib/moment-timezone-with-data.js"/>
        </xpath>
    </template>

</odoo>
